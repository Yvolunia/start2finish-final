{
  "name": "start2finish",
  "version": "1.0.0",
  "description": "Start to Finish App",
  "main": "server.js",
  "dependencies": {
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "cordova": "^8.0.0",
    "express": "^4.16.3",
    "mongoose": "^5.0.15",
    "nodemon": "^1.17.3",
    "react-router-dom": "^4.2.2"
  },
  "scripts": {
    "server": "node server.js",
    "client": "node scripts/start-client.js",
    "start": "concurrently \"npm run client:install && npm run build\" \"npm run server\"",
    "start:dev": "concurrently \"nodemon server.js\" \"npm run client\"",
    "client:install": "cd client && npm run install",
    "build": "node scripts/build.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "seed": "node scripts/seedDB.js"
  }
}
